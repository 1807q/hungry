.Mask{
  width: 100%;
  min-height: 1206rpx;
  background: black;
  background: rgba(0,0,0,0.6)
}
.header{
  width: 100%;
  height: 300rpx;
  /* background: red; */
  text-align: center;
  line-height:250rpx; 
}
.header>text{
  font-size: 35rpx;
  font-weight: bold;
  color: white;
}
.header>view{
  width: 380rpx;
  height: 50rpx;
  /* background: yellow; */
  margin-left: 180rpx;
  margin-top: -60rpx;
  display: flex;
  justify-content: space-between;
}
.header>view>image{
  width: 42rpx;
  height: 42rpx;
  display: block;
}
.conter{
  width: 100%;
  height: 385rpx;
  /* background: red; */
  margin: auto;
  padding-left: 35rpx;
}
.conter>view:nth-of-type(1){
  width: 90%;
  height: 50rpx;
  /* background: yellow; */
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: white;
  font-weight: bold;
}
.conter>view:nth-of-type(1)>text{
  width: 228rpx;
  height: 2rpx;
  background: #636770;
  display: block;
}
.conter>view:nth-of-type(2){
  width: 80%;
  /* height: 35rpx; */
  /* background: red; */
  margin-left: 80rpx;
}
.conter>view:nth-of-type(2)>view{
  width: 100%;
  height: 35rpx;
  font-size: 28rpx;
  color: white;
  /* line-height: 30rpx;/ */
  /* background: yellow; */
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.conter>view:nth-of-type(2)>view>image{
  width: 35rpx;
  height: 35rpx;
}
.conter>view:nth-of-type(2)>view>text{
  margin-left: 40rpx;
}
